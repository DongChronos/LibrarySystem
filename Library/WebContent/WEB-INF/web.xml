<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>Library</display-name>
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
    <welcome-file>login.html</welcome-file>
    <welcome-file>login.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <listener>
    <listener-class>com.Library.listener.OnLineListener</listener-class>
  </listener>
  <session-config>
    <session-timeout>10</session-timeout>
  </session-config>
  <filter>
    <filter-name>SetCharacterEncoding</filter-name>
    <filter-class>com.Library.filter.SetCharacterEncodingFilter</filter-class>
    <init-param>
      <param-name>encoding</param-name>
      <param-value>utf-8</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>SetCharacterEncoding</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>UserFilter</filter-name>
    <filter-class>com.Library.filter.UserFilter</filter-class>
    <init-param>
      <param-name>check</param-name>
      <param-value>/login.jsp;/login.jhtml;/login.html;/code.jhtml;/code.html;/code.jsp;/register.jsp;/register.html;/register.jhtml;/create_account_success.jsp</param-value>
    </init-param>
    <init-param>
      <param-name>error_url</param-name>
      <param-value>/login.jsp</param-value>
    </init-param>
    <init-param>
      <param-name>userConstants</param-name>
      <param-value>user</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>UserFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>PowerFilter</filter-name>
    <filter-class>com.Library.filter.PowerFilter</filter-class>
    <init-param>
      <param-name>lowPower</param-name>
      <param-value>/index.jsp</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>PowerFilter</filter-name>
    <url-pattern>/admin.jsp</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>PowerFilter</filter-name>
    <url-pattern>/admin.jhtml</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>PowerFilter</filter-name>
    <url-pattern>/admin.html</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>PowerFilter</filter-name>
    <url-pattern>/return.html</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>PowerFilter</filter-name>
    <url-pattern>/return.jhtml</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>PowerFilter</filter-name>
    <url-pattern>/return.jsp</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>PowerFilter</filter-name>
    <url-pattern>/admin_show.jhtml</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>PowerFilter</filter-name>
    <url-pattern>/admin_show.jsp</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>PowerFilter</filter-name>
    <url-pattern>/admin_show.html</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>PowerFilter</filter-name>
    <url-pattern>/insertApply.jhtml</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>PowerFilter</filter-name>
    <url-pattern>/insertApply.jsp</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>PowerFilter</filter-name>
    <url-pattern>/insertApply.html</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>PowerFilter</filter-name>
    <url-pattern>/admin_apply.jhtml</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>PowerFilter</filter-name>
    <url-pattern>/admin_apply.jsp</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>PowerFilter</filter-name>
    <url-pattern>/admin_apply.html</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>RegisterAction</servlet-name>
    <servlet-class>com.Library.action.RegisterAction</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegisterAction</servlet-name>
    <url-pattern>/register.jhtml</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LoginAction</servlet-name>
    <servlet-class>com.Library.action.LoginAction</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginAction</servlet-name>
    <url-pattern>/login.jhtml</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>CodeAction</servlet-name>
    <servlet-class>com.Library.action.CodeAction</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CodeAction</servlet-name>
    <url-pattern>/code.jhtml</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>EditorAction</servlet-name>
    <servlet-class>com.Library.action.EditorAction</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EditorAction</servlet-name>
    <url-pattern>/editor.jhtml</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LogoutAction</servlet-name>
    <servlet-class>com.Library.action.LogoutAction</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogoutAction</servlet-name>
    <url-pattern>/logout.jhtml</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SearchAction</servlet-name>
    <servlet-class>com.Library.action.SearchAction</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SearchAction</servlet-name>
    <url-pattern>/search.html</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AdminShowAction</servlet-name>
    <servlet-class>com.Library.action.AdminShowAction</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AdminShowAction</servlet-name>
    <url-pattern>/admin_show.jhtml</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>BorrowAction</servlet-name>
    <servlet-class>com.Library.action.BorrowAction</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BorrowAction</servlet-name>
    <url-pattern>/borrow.jhtml</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>BookDetailAction</servlet-name>
    <servlet-class>com.Library.action.BookDetailAction</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BookDetailAction</servlet-name>
    <url-pattern>/book_detail.jhtml</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ReturnAction</servlet-name>
    <servlet-class>com.Library.action.ReturnAction</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ReturnAction</servlet-name>
    <url-pattern>/return.jhtml</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ApplyAction</servlet-name>
    <servlet-class>com.Library.action.ApplyAction</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ApplyAction</servlet-name>
    <url-pattern>/apply.html</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>DeleteApplyAction</servlet-name>
    <servlet-class>com.Library.action.DeleteApplyAction</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DeleteApplyAction</servlet-name>
    <url-pattern>/delete_apply.html</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>AdminApplyAction</servlet-name>
  	<servlet-class>com.Library.action.AdminApplyAction</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>AdminApplyAction</servlet-name>
  	<url-pattern>/admin_apply.jhtml</url-pattern>
  </servlet-mapping>
  <context-param>
    <param-name>ok_url</param-name>
    <param-value>/index.jsp</param-value>
  </context-param>
  <context-param>
    <param-name>ad_url</param-name>
    <param-value>/admin.jsp</param-value>
  </context-param>
</web-app>